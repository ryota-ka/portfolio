{
  "version": "0.0.0",
  "description": "ryota-ka's portfolio",
  "main": "index.js",
  "repository": "git@github.com:ryota-ka/portfolio.git",
  "author": "Ryota Kameoka <kameoka.ryota@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.28.5",
    "koa": "^2.3.0",
    "koa-static": "^4.0.1",
    "node-fetch": "^1.7.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@types/koa": "^2.0.39",
    "@types/koa-static": "^3.0.0",
    "@types/node-fetch": "^1.6.7",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "@types/xmldom": "^0.1.29",
    "browser-sync": "^2.18.13",
    "browserify": "^14.4.0",
    "envify": "^4.1.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "stylus": "^0.54.5",
    "tsify": "^3.0.1",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.27",
    "watchify": "^3.9.0"
  },
  "name": "portfolio",
  "scripts": {
    "start": "npm-run-all --parallel watch:*",
    "watch:front": "watchify src/script.ts -o public/script.js -p [ tsify --target es5 ]",
    "watch:server": "nodemon --delay 0.5 dest/server/index.js",
    "watch:stylus": "stylus --watch style.styl -o public/style.css",
    "watch:sync": "browser-sync start -f ./public --reload-debounce 500 --port 3000 -p localhost:8080",
    "watch:tsc": "tsc --watch"
  }
}
